<?xml version="1.0" encoding="UTF-8"?>
<faces-config>
  <faces-config-extension>
    <namespace-uri>http://frostillic.us/bootstrap</namespace-uri>
    <default-prefix>bs</default-prefix>
  </faces-config-extension>
  <composite-component>
    <component-type>controlGroup</component-type>
    <composite-name>controlGroup</composite-name>
    <composite-file>/controlGroup.xsp</composite-file>
    <composite-extension>
      <designer-extension>
        <in-palette>true</in-palette>
        <category>Bootstrap</category>
      </designer-extension>
    </composite-extension>
    <property>
      <property-name>label</property-name>
      <property-class>string</property-class>
    </property>
    <property>
      <property-name>sublabel</property-name>
      <property-class>string</property-class>
    </property>
  </composite-component>
</faces-config>